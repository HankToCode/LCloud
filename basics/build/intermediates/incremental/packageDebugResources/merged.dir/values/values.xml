    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.DiverLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:dividerHeight">0.5dp</item>
        <item name="android:divider">@color/color_division_line</item>
        <item name="android:background">@color/color_division_line</item>
    </style>
    <style name="AppTheme.Label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/textSize_content</item>
        <item name="android:textColor">@color/color_gray</item>
    </style>
    <style name="AppTheme.Label.Description">
        <item name="android:gravity">top</item>
        <item name="android:lineSpacingExtra">5dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/textSize_content</item>
        <item name="android:singleLine">false</item>
    </style>
    <style name="AppTheme.Label.Layout">
        <item name="android:paddingTop">@dimen/padding_common_v</item>
        <item name="android:paddingBottom">@dimen/padding_common_v</item>
        <item name="android:paddingLeft">@dimen/padding_common_h</item>
        <item name="android:paddingRight">@dimen/padding_common_h</item>
    </style>
    <style name="AppTheme.Label.Title">
        <item name="android:textColor">@color/color_black</item>
        <item name="android:textSize">@dimen/textSize_titleSmall</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.NoActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.NoActionBar.FullScreen">
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="AppTheme.Toolbar" parent="Widget.AppCompat.ActionBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>

    </style>
    <declare-styleable name="CircleImageView">
        <attr name="type"/>
        <attr name="borderColor"/>
        <attr name="borderWidth"/>
        <attr name="rectRoundRadius"/>
    </declare-styleable>
    <declare-styleable name="ExpandableTextView">
        <attr format="integer" name="maxCollapsedLines"/>
        <attr format="integer" name="animDuration"/>
        <attr format="float" name="animAlphaStart"/>
        <attr format="reference" name="expandDrawable"/>
        <attr format="reference" name="collapseDrawable"/>
    </declare-styleable>
    <declare-styleable name="LoadingView">
        <!--  设置外面进度条的半径 -->
        <attr format="dimension|reference" name="outerCircleRadius"/>
        <!--  设置外面进度条的颜色 -->
        <attr format="color|reference" name="outerCircleColor"/>
        <!--  设置三角形外接圆的半径 -->
        <attr format="dimension|reference" name="innerTriangleRadius"/>
        <!--  设置三角形颜色 -->
        <attr format="color|reference" name="innerTriangleColor"/>
        <!--  设置背景的颜色 -->
        <attr format="color|reference" name="backgroundColor"/>
        <!--  设置是否需要背景 -->
        <attr format="boolean" name="isNeedBackground"/>
        <attr format="dimension|reference" name="strokeWidth"/>
    </declare-styleable>
</resources>